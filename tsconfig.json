  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "screens/*": ["./src/screens/*"],
      "store/*": ["./src/store/*"],
      "components/*": ["./src/components/*"],
      "assets/*": ["./src/assets/*"],
      "hooks/*": ["./src/hooks/*"],
      "utils/*": ["./src/utils/*"],
      "actions/*": ["./src/actions/*"],
      "navigators/*": ["./src/navigators/*"],
      "reducers/*": ["./src/reducers/*"],
      "global/*": ["./src/global/*"],
      "context/*": ["./src/context/*"],
      "underscore": ["lodash"]
